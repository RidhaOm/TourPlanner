<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.technikum.tourplanner.view.TourLogView"> <!---->
   <VBox prefWidth="704.0">
      <children>
          <HBox prefHeight="25.0" prefWidth="300.0">
              <children>
                  <Button id="tour-logs-button" mnemonicParsing="false" prefHeight="25.0" prefWidth="88.0" text=" Tour Logs" />
                  <Button mnemonicParsing="false" prefHeight="25.0" prefWidth="540.0" visible="false" />
                  <Button id="add-tour-log-button" fx:id="addTourLogButton" onAction="#openAddTourLogWindow" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" text="+" />
                  <Button id="delete-tour-log-button" fx:id="deleteTourLogButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" text="-" />
                  <Button id="modify-tour-log-button" fx:id="modifyTourLogButton" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" text="..." />
              </children>
          </HBox>
          <ListView id="tourLogListView" fx:id="tourLogListView" onMouseClicked="#selectTourLog" prefHeight="524.0" prefWidth="300.0" />

      </children>
   </VBox>


</AnchorPane>
